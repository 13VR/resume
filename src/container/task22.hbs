<main>
  <h1 class='text-center'>{{page_header.title}}</h1>
  <p
    class='text-center text-uppercase'
  >{{page_header.description}}</p>
  <div class='card card-body'>
    {{#with section}}
      {{#with about_us}}
        {{#with title_list}}
          <div class='display-6 text-end'>{{main}}</div>
          <br />
          <h2 class='text-end'>{{sub}}</h2>
          <br />
        {{/with}}
        <div>
          {{#with button}}
            <button
              type='button'
              class='btn btn-outline-info'
            >{{text}}</button>
          {{/with}}
        </div>
      {{/with}}    
  </div>
  <br>
    {{#with advantage}}
    <div class='card card-body'>
      <div class='display-6 text-muted'>{{title}}</div>
      <br>
        {{#with efficiency}}
        <div class='card card-body'>
          <div class='display-5 text-muted'>{{title}}</div><br>
          <h6 class="text-dark">{{sub_title}}</h6><br>
          <p class="text-dark">{{description}}</p>
          <div>
            <button type="btn" class="btn btn-outline-warning">{{button}}</button>
          </div>
        </div>
        {{/with}}
        <br>
         {{#with innovation}}
        <div class='card card-body'>
          <div class='display-5 text-muted text-center'>{{title}}</div><br>
          <h6 class="text-dark text-center">{{sub_title}}</h6><br>
          <p class="text-dark text-center">{{description}}</p>
          <div>
            <button type="btn" class="btn btn-outline-warning">{{button}}</button>
          </div>
        </div>
        {{/with}}
        <br>
        {{#with innovation}}
        <div class='card card-body'>
          <div class='display-5 text-muted text-end'>{{title}}</div><br>
          <h6 class="text-dark text-end">{{sub_title}}</h6><br>
          <p class="text-dark text-end">{{description}}</p>
          <div>
            <button type="btn" class="btn btn-outline-warning">{{button}}</button>
          </div>
        </div>
        {{/with}}
    </div>
    {{/with}}
    <br>
    {{#with agreement}}
    <div class="card card-body">
      <div class='display-5'>{{title}}</div>
      <br>
      {{#with info}}
      <div class="card card-body">      
        <h5>{{sub_title}}</h5>      
      
      <br>
      {{#with detail}}
      <div class="card card-body">      
        <p>{{text}}</p> 
        <br>
        <div>
          <button type="btn" class="btn btn-success">
            {{button}}
          </button>     
        </div>
      </div>
      {{/with}}
      </div>
      {{/with}}
    </div>
    {{/with}}
  {{/with}}
</main>