<div>
  <main>
    <div class='card mb-3'>
      <img
        src='data:image/jpeg;base64,'
        class='card-img-top'
        alt='toyota'
      />
      <div class='card-body'>
        <h5 class='card-title'><b>{{make}}
            {{model}}
            -
            {{year}}, color -
            {{color}}</b></h5>
            <br>
        {{#with features}}
             {{#with interior}}
            
              <div><b>Interior:</b></div>
                  <hr>
                  {{#with seats}}
                    <p class='card-text'>Seats:
                      {{material}}, color -
                      {{color}}
                      ({{#if heated}}
                        heated &
                      {{/if}}
                      {{#if ventilated}}
                        ventilated.
                      {{/if}})
                  {{/with}} 
                {{#with dashboard}}    
                  <div class='card-text'>Dashboard:
                    {{material}}, color -
                    {{color}}.
                    <div> Display: 
                    {{#with display}}
                      type ({{type}}), {{size}}, {{resolution}}
                      {{#if touchscreen}}
                      , touchscreen.
                      {{/if}}
                    {{/with}}
                    </div>
                {{/with}}
                <div>Audio system:
                {{#with audio}}
                  {{system}}, speakers - {{speakers}} 
                  
                    {{#if subwoofer}}
                    <span class="badge text-bg-success">subwoofer</span>
                    {{/if}}
                      {{#if bluetooth}}
                    <span class="badge text-bg-warning"> bluetooth</span>
                    {{/if}}
                      {{#if USB}}
                    <span class="badge text-bg-light"> USB</span>
                    {{/if}}              
                {{/with}}
                 </div>
                {{/with}}    
              </div>           
            
          <br><br>
          <div><b>Exterior:</b></div>
          <hr>
          {{#with exterior}}
            {{#with wheels}}
              <div>Wheels: radius - {{size}}, type - {{type}}, color - {{color}}</div>
            {{/with}}
            {{#with headlights}}
              <div>Headlights: {{type}}, brightness - {{brightness}} {{#if automatic}}, ragulated - automatic {{/if}}</div>
            {{/with}}
            {{#with sunroof}}
              <div>Sunroof: {{type}}, {{size}}, {{#if automatic}}, ragulated - automatic {{/if}}</div>
            {{/with}}
          {{/with}}
          <br><br>
          <div><b>Safaty airbags:</b></div> 
          <hr>
          {{#with safety}}
            {{#with airbags}}
              <ul>
                <li>Front - <span class="badge text-bg-danger">{{front}}</span></li>
                <li>Side - <span class="badge text-bg-danger">{{side}}</span></li>
                <li>Knee - <span class="badge text-bg-danger">{{knee}}</span></li>
                <li>Rear - <span class="badge text-bg-danger">{{rear}}</span></li>
              </ul>
            {{/with}}
            <br><br>
            <div><b>Assistances:</b></div> 
            <hr>
            {{#with assistance}}
              <ul>
                <li>{{#if blind_spot_monitoring}}  <span class="badge text-bg-success">Blind spot monitoring </span> {{/if}}</li>
                <li>{{#if rear_cross_traffic_alert}}  <span class="badge text-bg-success">Rear cross traffic alert </span> {{/if}}</li>
                <li>{{#if lane_departure_warning}}  <span class="badge text-bg-success">Lane departure warning </span> {{/if}}</li>
                <li>{{#if adaptive_cruise_control}}  <span class="badge text-bg-success">Adaptive cruise control </span> {{/if}}</li>
                <li>{{#if collision_warning}}  <span class="badge text-bg-success">Collision warning </span> {{/if}}</li>
              </ul>
            {{/with}}                 
          {{/with}}
        {{/with}}
        <br><br>
        <div><b>Engine:</b></div>
        <hr>
        {{#with engine}}
          <table class="table table-bordered">
            <tbody>
              <tr>
                <th>Type</th>
                <td>{{type}}</td>
              </tr>
              <tr>
                <th>Displacement</th>
                <td>{{displacement}}</td>
              </tr>
              <tr>
                <th>Horsepower</th>
                <td>{{horsepower}}</td>
              </tr>
              <tr>
                <th>Torque</th>
                <td>{{torque}}</td>
              </tr>
              {{#with transmission}}
              <tr>                
                <th>Transmission</th>
                <td>{{type}}, {{gears}} - gears</td>               
              </tr>
               {{/with}}
            </tbody>
          </table>
          {{/with}}
          <br>
          <div><b>Fuel economy:</b></div>
        <hr>
        {{#with fuel_economy}}
          <ul>
                <li>City - <span class="badge text-bg-danger">{{city}}</span></li>
                <li>Highway - <span class="badge text-bg-danger">{{highway}}</span></li>
                <li>Combine - <span class="badge text-bg-danger">{{combined}}</span></li>                
              </ul>
        {{/with}}
        {{#with price}}
          <table class="table table-bordered">
            <tbody>
              <tr>
                <th>Name</th>
                <th>Prices</th>
              </tr>
            </tbody>
            <tbody>
              <tr>
                <th>Base</th>
                <td>{{base}} $</td>
              </tr>
              <tr>
                <th>Destination</th>
                <td>{{destination}} $</td>
              </tr>
              {{#with options}}
                <tr>
                  <th>Navigation</th>
                  <td>{{navigation}} $</td>
                </tr>
                <tr>
                  <th>Noonroof</th>
                  <td>{{moonroof}} $</td>
                </tr>                
                <tr>                
                  <th>Premium paint</th>
                  <td>{{premium_paint}} $</td>               
                </tr>               
              {{/with}}
            </tbody>
            <tfoot>
              <tr>
                <th>TOTAL:</th>
                <th>{{total}}</th>
              </tr>
            </tfoot>
          </table>
        {{/with}}
        </p>      
      </div>
    </div>
  </main>
</div>

          
        