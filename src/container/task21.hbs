<main>

  {{#with heading}}
    <div class='card' style='width: 75rem;'>
      <div class='card-body'>
        <div class='card'>
          <div class='card-body'>
            {{#with main}}
              <h1>{{title}}</h1>
              <p>{{text}}</p>
              <div><button
                  class='btn btn-primary'
                >{{button_text.solid}}</button>
              </div>
              <br />
              <br />
              <div><button
                  class='btn btn-outline-primary'
                >{{button_text.outline}}</button>
              </div>
            {{/with}}
          </div>
        </div>
        <br />
        <div class='card'>
          <div class='card-body'>
            {{#with second}}
              <h2>{{title}}</h2>
              <p>{{text}}</p>
              <div><button
                  class='btn btn-secondary'
                >{{button_text.solid}}</button>
              </div>
              <br />

              <div><button
                  class='btn btn-outline-secondary'
                >{{button_text.outline}}</button>
              </div>
            {{/with}}
          </div>
        </div>
        <br />
        <div class='card'>
          <div class='card-body'>
            {{#with subblock}}
              <h3>{{title}}</h3>
              <p>{{text}}</p>
              <div><button
                  class='btn btn-dark'
                >{{button_text.solid}}</button>
              </div>
              <br />

              <div><button
                  class='btn btn-outline-dark'
                >{{button_text.outline}}</button>
              </div>
            {{/with}}
          </div>
        </div>
      </div>
    </div>
  {{/with}}
  <br />
  <br />
  {{#with nested_display}}
    <div class='card' style='width: 75rem;'>
      <div class='card-body'>
        <div class='display-1'>{{title}}</div>
        <br />
        <div class='card' style='width: 50rem;'>
          <div class='card-body'>
            {{#with children_display}}
              <div class='display-4'>{{title}}</div>
              <br />
              <p>{{text}}</p>
              <div><button
                  class='btn btn-outline-warning'
                >{{button.text}}</button>
              </div>
            {{/with}}
          </div>
        </div>
        <br />
        <h6>{{header_text}}</h6>
        <br />
        <div><button
            class='btn btn-info'
          >{{button.text}}</button>
        </div>
      </div>
    </div>
  {{/with}}
  <br />
  <br />
  {{#with paragraph}}
    <div class='card' style='width: 75rem;'>
      <div class='card-body'>
        <p class='text-uppercase'>{{title}}</p>
        <p class='text-lowercase'>{{text}}</p>
        <a
          href='{{button.url}}'
          class='btn btn-link'
          target='_blank'
        >{{button.text}}</a>
        <br /><br />
        <div class='card' style='width: 70rem;'>
          <div class='card-body'>
            <br />
            {{#each list_name}}
              <ul>
                <li>
                  <a
                    href='{{url}}'
                    class='btn btn-link'
                    target='_blank'
                  >{{text}}</a>
                </li>
              </ul>
            {{/each}}
          </div>
        </div>
      </div>
    </div>
  {{/with}}
</main>