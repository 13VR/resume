<div>
  {{#with person}}
    <main>
      <section>
        <h1>Person: {{name}}</h1><hr /><br />
        <div>Age: {{age}}</div><br />
        <div>Gender: {{gender}}</div><br />
        <address>Address:
          {{#with address}}
            {{country}},
            {{zip}},
            {{state}}
            state,
            {{city}}
            city,
            {{street}}
          {{/with}}
        </address>
        <br />
        <hr />
      </section>
      <section>
        {{#each education}}
        <details open>
          <summary><u>Education</u></summary><br>
          <div>University: <cite>{{university}}</cite></div>
          <br />
          <div>Speciality: <em>{{major}}</em></div><br />
          <div>Degree: <em>{{degree}}</em></div><br />
          <div>Year of graduation:
            <time
              datatime='2012'
            >{{graduationYear}}</time></div>
        </details>
        {{/each}}
        <br />
        <hr />
      </section>
      <section>
        {{#each workExperience}}
        <details open>
          <summary><u>Work Experience</u></summary><br>
          <div>Company:
            {{> link href="https://google.com" text=company blank=true noLine=true}}
          </div><br>
          <div>Was founded in -{{year_founded}}</div>
          <div>Industry: {{industry}}</div><br>
          <div><b>My speciality: {{title}}</b></div><br>
          <dev>I was responsible for: <q>{{responsibilities}}</q></dev><br><br>
          <div>Working period: <time> {{startDate}} - {{endDate}}</time></div><br>
          <ul>
            {{#each employees}}
            <li>
                <div><b>Projects: {{projects.[0].name}}</b></div><br>
                <div>Description: <em>{{projects.[0].description}}</em></div>
                <div>Work status: {{projects.[0].status}}</div><br>
                <div>Headmaster: {{name}}</div>
                <div>Position: {{position}}</div>
                <div>Departmenr: {{department}}</div><br>
                {{#each projects}}
                    {{#each teams}}
                        <div>Team name: <mark>{{team_name}}</mark></div>
                        <div>Members:</div><br>
                        <ul>
                            <li>
                                <div>{{team_leader.title}} - {{team_leader.name}}</div>
                                <div>Email: {{> link text=team_leader.email href=team_leader.email blank=true noLine=true}}</div>
                            </li><br>
                            {{#each team_members}}
                            <li>
                                <div>{{title}} - {{name}}</div>
                                <div>Email: {{> link text=email href=email blank=true noLine=true}}</div>
                                <div>PRO Skills: {{skills}}</div>
                                <br>
                                <ul>
                                    <li>                            
                                        {{#each projects}}
                                            <div><b>{{name}}</b></div><br>
                                            <div>Description: <em>{{description}}</em></div>
                                            <div><u>Technologies:</u> {{technologies}}</div><br>
                                            <div>Members:</div><br>
                                            <ul>
                                                {{#each team_members}}
                                                <li><div>{{title}} - {{name}}</div></li>                                               
                                                {{/each}}
                                            </ul>
                                            <hr>
                                        {{/each}}                                           
                                    </li>
                                </ul>
                            </li>
                            {{/each}}
                        </ul>
                    {{/each}}
                {{/each}}
            </li>
            
            {{/each}}
          </ul>
        </details>
        {{/each}}
      </section>
    </main>
  {{/with}}
</div>
